<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Order of components for each drawable array:
        Centipede Head Above
        Centipede Head Below
        Centipede Body Above
        Centipede Body Below
        Lawn Top
        Lawn Bottom -->

    <array name="sunny_day" tools:keep="@array/sunny_day">
        <item>@drawable/centipede_head_above_sunny_day</item>
        <item>@drawable/centipede_head_below_sunny_day</item>
        <item>@drawable/centipede_body_above_sunny_day</item>
        <item>@drawable/centipede_body_below_sunny_day</item>
        <item>@drawable/lawn_top_sunny_day</item>
        <item>@drawable/lawn_bottom_sunny_day</item>
    </array>

